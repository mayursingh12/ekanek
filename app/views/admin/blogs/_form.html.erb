<%= simple_form_for [:admin, @blog] do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :file, input_html: { class: 'form-control' }  %>
  <br>
  <%= f.submit 'Save', class: 'btn btn-outline-primary' %>
  <%= link_to 'List', admin_blogs_path, class: 'btn btn-outline-info' %>
<% end %>